{
  "fields": [
    {
      "type": "pathbrowser",
      "browserRoot": "/content/assets",
      "placeholder": "logo",
      "label": "Logo",
      "model": "logo"
    },
    {
      "type": "input",
      "inputType": "url",
      "placeholder": "logourl",
      "label": "Logo Url",
      "model": "logourl"
    },
    {
      "type": "input",
      "inputType": "range",
      "placeholder": "logosize",
      "label": "Logo Size",
      "model": "logosize",
      "min": 1,
      "max": 10
    },
    {
      "type": "materialswitch",
      "textOn": "yes",
      "textOff": "no",
      "valueOn": "true",
      "valueOff": "false",
      "placeholder": "collapsed",
      "label": "Collapse Menu",
      "model": "collapsed"
    },
    {
      "type": "materialswitch",
      "textOn": "yes",
      "textOff": "no",
      "valueOn": "true",
      "valueOff": "false",
      "placeholder": "sticky",
      "label": "Sticky",
      "model": "sticky"
    },
    {
      "type": "collection",
      "multifield": true,
      "fieldLabel": [
        "text",
        "value"
      ],
      "fields": [
        {
          "type": "input",
          "inputType": "text",
          "placeholder": "text",
          "label": "Link Text",
          "model": "text"
        },
        {
          "type": "pathbrowser",
          "browserRoot": "/content/assets",
          "placeholder": "link",
          "label": "Link Url",
          "model": "link"
        }
      ],
      "placeholder": "links",
      "label": "Links",
      "model": "links"
    },
    {
      "type": "collection",
      "multifield": true,
      "fieldLabel": [
        "buttontext",
        "value"
      ],
      "fields": [
        {
          "type": "input",
          "inputType": "text",
          "placeholder": "buttontext",
          "label": "Button Text",
          "model": "buttontext"
        },
        {
          "type": "pathbrowser",
          "browserRoot": "/content/assets",
          "placeholder": "buttonlink",
          "label": "Button Link",
          "model": "buttonlink"
        },
        {
          "type": "input",
          "inputType": "color",
          "placeholder": "buttoncolor",
          "label": "Button Color",
          "model": "buttoncolor"
        }
      ],
      "placeholder": "buttons",
      "label": "Buttons",
      "model": "buttons"
    },
    {
      "type": "material-radios",
      "values": [
        {
          "name": "Image",
          "value": "image"
        },
        {
          "name": "Color",
          "value": "color"
        },
        {
          "name": "Gradient",
          "value": "gradient"
        }
      ],
      "placeholder": "backgroundtype",
      "label": "Background Type",
      "model": "backgroundtype"
    },
    {
      "type": "pathbrowser",
      "browserRoot": "/content/assets",
      "placeholder": "bgimage",
      "label": "Background Image",
      "model": "bgimage",
      "visible": "model.backgroundtype == 'image'"
    },
    {
      "type": "materialswitch",
      "textOn": "yes",
      "textOff": "no",
      "valueOn": "true",
      "valueOff": "false",
      "placeholder": "overlay",
      "label": "Overlay",
      "model": "overlay",
      "visible": "model.backgroundtype == 'image'"
    },
    {
      "type": "input",
      "inputType": "color",
      "placeholder": "overlaycolor",
      "label": "Overlay Color",
      "model": "overlaycolor",
      "visible": "model.overlay == 'true' and model.backgroundtype == 'image'"
    },
    {
      "type": "input",
      "inputType": "range",
      "placeholder": "overlayopacity",
      "label": "Overlay opacity",
      "model": "overlayopacity",
      "visible": "model.overlay == 'true' and model.backgroundtype == 'image'",
      "max": 100
    },
    {
      "type": "input",
      "inputType": "color",
      "placeholder": "bgcolor",
      "label": "Background Color",
      "model": "bgcolor",
      "visible": "model.backgroundtype == 'color' or model.backgroundtype == 'gradient'"
    },
    {
      "type": "input",
      "inputType": "color",
      "placeholder": "color2",
      "label": "Color 2",
      "model": "color2",
      "visible": "model.backgroundtype == 'gradient'"
    },
    {
      "type": "input",
      "inputType": "range",
      "placeholder": "toppadding",
      "label": "Top Padding",
      "model": "toppadding",
      "visible": "model.fullheight != 'true'",
      "max": 120
    },
    {
      "type": "input",
      "inputType": "range",
      "placeholder": "bottompadding",
      "label": "Bottom Padding",
      "model": "bottompadding",
      "visible": "model.fullheight != 'true'",
      "max": 120
    }
  ]
}